{
	"name": "no-image",
	"id": "noimage",
	"version": "1.0.0",
	"description": "pro",
	"manifest_version": 2,
	"permissions": [
		"webRequest",
		"downloads",
		"tabs",
		"*://*/*"
	],
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content.js"],
		"run_at": "document_start"
	}],
	"icons": {
		"48": "icon.png"
	}
}